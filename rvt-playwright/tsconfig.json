{
    "compilerOptions": {
      "target": "es2018",
      "module": "ESNext",
      "moduleResolution": "Node",
      "declaration": true,
      "declarationMap": true,
      "composite": true,
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      
    },
    "ts-node": {
         "esm": true
     }
}
  